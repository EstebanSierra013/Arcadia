<div class="container card-list p-2">
  <div class="d-flex justify-content-around">
    <div class="col col-md-7 d-flex align-items-center">
      <a href="" id="<%= objet.Nom%>" onclick="showInfo(event); return false">
        <img src="<%= objet.Image %>" class="d-block w-100" alt="Image <%= objet.Nom%>">
      </a>      
    </div>
    <div id="<%= objet.Nom%>-info" style="display: none;"class="col col-md-5 p-2 gap-5 flex-column justify-content-center text-center">
      <h3 class="text-center my-2 my-md-3 my-lg-4 w-100"><%= objet.Nom %></h3>
      <h4 class="fw-bolder text-start ms-5">Race:</h4><p class="text-start ms-5"><%= objet.Race %></p>
    </div>
  </div>
</div>